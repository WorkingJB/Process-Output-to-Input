        <script>
            $(document).ready(function () {
        // Activate accessible dropdowns
        $(".nav-user, .nav-browse").accessibleDropDownMenu();
    });

    $.fn.accessibleDropDownMenu = function (settings) {

        const $navContainer = $(this);
        const $top_level_links = $navContainer.find('.drop-group');
        const $drop_submenu = $(".drop-submenu");

        settings = jQuery.extend({
            isActiveClass: 'is-active',
            keyTab: 9,
            keyArrowRight: 39,
            keyArrowLeft: 37,
        }, settings);

        // Adding aria-haspopup and aria-expanded for appropriate items
        $top_level_links.each(function () {
            if ($(this).find('ul').length > 0) {
                $(this).attr({ "aria-haspopup": true, "aria-expanded": false });

                // find submenu
                if ($(this).find(".drop-more").length > 0) {
                    $(this).find(".drop-more").attr({ "aria-haspopup": true, "aria-expanded": false });
                }
            }
        });

        $navContainer.on('focusin', '.drop-group > a', function (e) {
            $(this)
                .parent('.drop-group')
                .addClass(settings.isActiveClass)
                .attr("aria-expanded", true)
                .siblings('.drop-group')
                .removeClass(settings.isActiveClass)
                .attr("aria-expanded", false);
        });

        // Hide menu if focus occurs outside of navigation
        $navContainer.find('a:last').keydown(function (e) {
            if (e.keyCode === settings.keyTab) {
                // If the user tabs out of the navigation hide all menus
                $(this)
                    .attr('aria-expanded', false)
                    .removeClass(settings.isActiveClass);
                $(this)
                    .attr('aria-expanded', false)
                    .removeClass(settings.isActiveClass)
                $navContainer.find('.drop-group')
                    .removeClass(settings.isActiveClass)
                    .attr("aria-expanded", false);
            }
        });

        // If key is pressed while on the last link in a sub menu
        $navContainer.on('focusin', '.drop-more > a', function (e) {
            $(this)
                .parent('.drop-more')
                .addClass(settings.isActiveClass)
                .attr("aria-expanded", true)
                .siblings('.drop-more')
                .removeClass(settings.isActiveClass)
                .attr("aria-expanded", false);
        });

        // Tabbing sub menu
        $drop_submenu.on('focusin', 'a', function (e) {
            const $parent = $(this).parent('li');

            if ($parent.is('li:last-child')) {
                $(this).on("keydown", function () {
                    $(this).parents('.drop-more')
                        .attr('aria-expanded', false)
                        .removeClass(settings.isActiveClass);
                });
            }
        });

        // Navigate top level links using arrow left/right keys
        $top_level_links.on("keyup", function (e) {
            if (e.keyCode === settings.keyArrowRight) {
                $(this)
                    .removeClass(settings.isActiveClass)
                    .attr("aria-expanded", false);

                // focus
                $(this)
                    .next('li')
                    .find("a")
                    .first()
                    .focus();
            }

            if (e.keyCode === settings.keyArrowLeft) {
                $(this)
                    .removeClass(settings.isActiveClass)
                    .attr("aria-expanded", false);
                // focus
                $(this)
                    .prev('li')
                    .find("a")
                    .first()
                    .focus();
            }
        });

        // Hide if click occurs outside of navigation
        $(document).click(function () {
            $top_level_links.attr('aria-expanded', false).removeClass(settings.isActiveClass)
        });
    }
        </script>

